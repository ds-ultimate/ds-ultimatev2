{
    "name": "dsultimate/main",
    "type": "project",
    "description": "The ultimate Staemme statistics site",
    "keywords": [
        "dsltimate",
        "ds-ultimate",
        "staemme"
    ],
    "license": "",
    "require": {
        "php": "^8.2.0",
        "barryvdh/laravel-translation-manager": "^0.6.1",
        "dsultimate/click-captcha-laravel": "@dev",
        "guzzlehttp/guzzle": "^7.4",
        "hypoid/lavacharts": "^3.2",
        "laravel-notification-channels/discord": "^1.1",
        "laravel/framework": "^12.0",
        "laravel/socialite": "^5.0",
        "laravel/ui": "^4.0",
        "rap2hpoutre/laravel-log-viewer": "^2.0",
        "socialiteproviders/discord": "^4.0",
        "spatie/laravel-cookie-consent": "^3.0",
        "yajra/laravel-datatables-oracle": "~12.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "laravel/tinker": "^2.0",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.5"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ds-ultimate/click-captcha-laravel.git"
        },
	{
		"type": "vcs",
		"url": "https://github.com/martialus87/lavacharts"
	}
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
