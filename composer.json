{
    "name": "dsultimate/main",
    "type": "project",
    "description": "The ultimate Staemme statistics site",
    "keywords": [
        "dsltimate",
        "ds-ultimate",
        "staemme"
    ],
    "license": "",
    "require": {
        "php": "^8.0.0",
        "barryvdh/laravel-translation-manager": "^0.6.1",
        "buzz/laravel-google-captcha": "^2.1",
        "doctrine/dbal": "dev-dev-3.x.x",
        "google/recaptcha": "^1.2",
        "khill/lavacharts": "3.1.*",
        "laravel-notification-channels/discord": "^1.1",
        "laravel/framework": "^8.0",
        "laravel/socialite": "^5.0",
        "laravel/ui": "^3.0",
        "rap2hpoutre/laravel-log-viewer": "^2.0",
        "socialiteproviders/discord": "^4.0",
        "spatie/laravel-cookie-consent": "^3.0",
        "yajra/laravel-datatables-oracle": "~9.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "filp/whoops": "^2.0",
        "laravel/tinker": "^2.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ds-ultimate/dbal.git"
        }
    ],
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
